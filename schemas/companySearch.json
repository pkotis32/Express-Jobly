{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [],
    "properties": {
        "name": {
            "type": "string",
            "default": "",
            "title": "The q Schema",
            "minLength": 1,
            "examples": [
                "example search"
            ]
        },
        "minEmployees": {
            "type": "integer",
            "title": "The minEmployees Schema",
            "minimum": 0,
            "examples": [
                10
            ]
        },
        "maxEmployees": {
            "type": "integer",
            "title": "The maxEmployees Schema",
            "minimum": 0,
            "examples": [
                100
            ]
        }
    },
    "examples": [{
        "q": "example search",
        "minEmployees": 10,
        "maxEmployees": 100
    }]
}